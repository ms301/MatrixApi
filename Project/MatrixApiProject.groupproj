        <MSBuild Projects="..\Demo\MatrixDemo.dproj" Targets="Make"/>
    </Target>
    <Target Name="Build">
        <CallTarget Targets="CloudAPI;MatrixApi;MatrixDemo"/>
    </Target>
    <Target Name="Clean">
        <CallTarget Targets="CloudAPI:Clean;MatrixApi:Clean;MatrixDemo:Clean"/>
    </Target>
    <Target Name="Make">
        <CallTarget Targets="CloudAPI:Make;MatrixApi:Make;MatrixDemo:Make"/>
    </Target>
    <Import Project="$(BDS)\Bin\CodeGear.Group.Targets" Condition="Exists('$(BDS)\Bin\CodeGear.Group.Targets')"/>
</Project>
