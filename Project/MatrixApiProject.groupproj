    <Target Name="MatrixDemo" DependsOnTargets="MatrixApi">
        <MSBuild Projects="..\Demo\MatrixDemo.dproj"/>
    </Target>
    <Target Name="MatrixDemo:Clean" DependsOnTargets="MatrixApi:Clean">
